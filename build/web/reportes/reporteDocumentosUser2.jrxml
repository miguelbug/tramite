	<queryString><![CDATA[SELECT TD.NOMBRE_DOCU||' N°-'||DOIF.CORRELATIVO_DOCOFINT||'-'||DOIF.SIGLAS||'-'||TO_CHAR(DOIF.FECHA,'YYYY') AS DOCUMENTO,
DOIF.ASUNTO,
TO_CHAR(DOIF.FECHA,'DD/MM/YYYY HH:MI:SS') AS FECHA,
D1.NOMBRE AS ORIGEN,
D2.NOMBRE AS DESTINO,
USUA.USU_NOMBRE
FROM DOCUMENTOS_OFIINT DOIF, USUARIO USUA, TIPOS_DOCUMENTOS TD, DEPENDENCIA D1, DEPENDENCIA D2
WHERE DOIF.ID_DOCUMENTO=TD.ID_DOCUMENTO
AND DOIF.CODIGO=D1.CODIGO
AND DOIF.CODIGO1=D2.CODIGO
AND DOIF.USU=USUA.USU
AND DOIF.USU=$P{usuario}
AND DOIF.ID_DOCUMENTO=$P{tipo}
AND DOIF.FECHA BETWEEN $P{fechain} AND $P{fechafin}
ORDER BY DOIF.CORRELATIVO_DOCOFINT DESC]]></queryString>

