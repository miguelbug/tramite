M1.NOMBRE AS ORIGEN,
M2.NOMBRE AS DESTINO,
to_char(DE.FECHA,'DD/MM/YYY HH24:MI:ss') as fecha,
USUA.USU_NOMBRE
from DOCUS_EXTINT DE, DEPENDENCIA M1, DEPENDENCIA M2, TIPOS_DOCUMENTOS TD, USUARIO USUA, OFICINA oficina
WHERE DE.CODIGO=M1.CODIGO
