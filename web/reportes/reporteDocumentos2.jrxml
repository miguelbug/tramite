    <queryString><![CDATA[SELECT DI.TRAM_NUM,
TO_CHAR(DI.FECHAREGISTRO,'DD/MM/YYYY') AS FECHA,
OFI.NOMBRE_OFICINA AS ORIGEN,
DI.DOCU_ASUNTO,
USUA.USU_NOMBRE
FROM DOCUS_INTERNOS DI, USUARIO USUA, OFICINA OFI, TIPOS_DOCUMENTOS TD
WHERE DI.USU=USUA.USU
AND USUA.ID_OFICINA=OFI.ID_OFICINA
AND DI.ID_DOCUMENTO=TD.ID_DOCUMENTO
AND TD.ID_DOCUMENTO=$P{tipo}
AND USUA.USU_NOMBRE=$P{usuario}
AND DI.FECHAREGISTRO BETWEEN $P{fechain} and $P{fechafin}
AND DI.TRAM_NUM IS NOT NULL
ORDER BY DI.TRAM_NUM DESC
]]></queryString>

