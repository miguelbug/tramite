	<queryString><![CDATA[SELECT DI.DOCU_NOMBREINT||' N°-'||DI.DOCU_CORRELAINT||'-'||DI.DOCU_SIGLASINT||'-'||DI.DOCU_ANIOINT as DOCUMENTO,
TO_CHAR(DI.FECHAREGISTRO,'DD/MM/YYYY HH:MI:SS') AS FECHA,
DI.TRAM_NUM AS NUMERO_TRAMITE,
TO_CHAR(DI.TRAM_FECHA,'DD/MM/YYYY HH:MI:SS') AS FECHA_ENVIO,
TO_CHAR(DI.FECHAREGISTRO,'DD/MM/YYYY HH:MI:SS') AS FECHAREGISTRO,
TO_CHAR(TM.FECHA_DERIVACION,'DD/MM/YYYY HH:MI:SS') AS FECHA_DERIVADO,
D1.NOMBRE AS ORIGEN,
D2.NOMBRE AS DESTINO,
USUA.USU_NOMBRE AS USUARIO,
DI.DOCU_ASUNTO AS ASUNTO
FROM DOCUS_INTERNOS DI, USUARIO USUA, TRAMITE_MOVIMIENTO TM, DEPENDENCIA D1, DEPENDENCIA D2
WHERE DI.TRAM_NUM=$P{tramnum}
AND DI.TRAM_FECHA=$P{fechaenvio}
AND TM.CODIGO=D1.CODIGO
AND TM.CODIGO1=D2.CODIGO
AND D2.CODIGO NOT IN('1001871','1001870','1001872','1001868','1001869')
AND DI.USU=$P{usuario}
AND DI.DOCU_CORRELAINT=$P{correlativo}
ORDER BY DI.DOCU_CORRELAINT DESC]]></queryString>

